import React, { Component } from 'react';

class App extends Component {
  render() {
    return (
    <div>
    <div>hello</div>
    <div>bye</div>
    </div>
    );
  }
}


// Fragment로도 감싸줄수 있음 (불 필요한 div가 사라짐) 
import React, { Component, Fragment } from 'react';

class App extends Component {
  render() {
    return (
    <Fragment>
    <div>hello</div>
    <div>bye</div>
    </Fragment>
    );
  }
}
